@{
    ViewBag.Title = "Configuración de campos";
    
}
<div id="migas">
    <p>Controles configurados </p>
</div>
@using (Ajax.BeginForm("GetIndex", "ControlEvents", null,
                new AjaxOptions { UpdateTargetId = "CustomerForm" },
                new { id = "EditCustomerProfileForm2" }))
{
    @Html.Hidden("Data_FormPageId", (Guid)ViewBag.Data_FormPageId)
    <div id="CustomerForm">
        @{Html.RenderPartial("_Index");}
    </div>


   

    @Html.Hidden("Data_FormPageName", (string)ViewBag.Data_FormPageName)
    @Html.Hidden("Data_FormId", (Guid)ViewBag.Data_FormId)
    @Html.ActionLink("Volver a la lista de páginas", "Edit", "Form", new { id = ViewBag.Data_FormId }, new { @class = "decorated" })

}
